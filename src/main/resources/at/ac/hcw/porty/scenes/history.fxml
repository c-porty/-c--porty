<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.ac.hcw.porty.controller.HistoryController"
            prefHeight="960.0" prefWidth="1184.0" styleClass="porty-content-pane">
    <HBox prefHeight="50.0" prefWidth="1184.0" styleClass="porty-top-bar">
        <ToggleButton fx:id="simplicityModeSwitch" mnemonicParsing="false" text="Simple/Advanced" styleClass="porty-switch-text" />
        <Region HBox.hgrow="ALWAYS" styleClass="porty-top-bar-spacer" />
        <ToggleButton fx:id="lightModeSwitch" mnemonicParsing="false" text="Theme" styleClass="porty-switch-icon"/>
        <Button fx:id="languageButton" mnemonicParsing="false" text="EN" styleClass="porty-top-bar-button"/>
    </HBox>
</VBox>
